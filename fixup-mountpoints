            -e 's block/bootdevice/by-name/vbmeta mmcblk0p7 ' \
            -e 's block/bootdevice/by-name/vendor mmcblk0p67 ' \
            -e 's block/bootdevice/by-name/xbl mmcblk0p40 ' \
            "$@"
        ;;
    *)
        cat <<EOF

****************************************************************
****************************************************************
ERROR: $DEVICE does not have mountpoint fixup data - see
       Sailfish OS HADK for details on how to fix this.
****************************************************************
****************************************************************

EOF
        exit 1
        ;;
esac

